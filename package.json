    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "nock": "^13.5.1",
    "npm-run-all": "^4.1.5",
    "parcel": "^2.10.1",
    "rimraf": "^5.0.5",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.2.2"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "dependencies": {
    "@cartesi/rollups": "^1.1.0",
