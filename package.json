  "description": "Web3 Client to Send Input to dApp",
  "source": "src/main.ts",
  "main": "dist/main.js",
  "type": "module",
  "scripts": {
    "watch": "parcel watch",
    "build:parcel": "parcel build --dist-dir dist/",
    "build": "run-s wagmi build:parcel",
